{
  "name": "mobile",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "start:coverage": "cross-env COVERAGE=true nyc --silent expo start --no-dev",
    "test:e2e": "maestro test test/.maestro/",
    "test:e2e:simple": "powershell -Command \"$env:EXPO_DEVTOOLS='false'; Start-Process npx -ArgumentList 'expo','start' -PassThru; Start-Sleep 15; maestro test test/.maestro/\"",
    "test:e2e:coverage": "powershell -Command \"$env:COVERAGE='true'; $env:NODE_ENV='test'; $env:EXPO_DEVTOOLS='false'; Start-Process npx -ArgumentList 'expo','start' -PassThru; Start-Sleep 15; maestro test test/.maestro/; nyc report\"",
    "coverage:report": "nyc report --reporter=html --reporter=text --reporter=lcov"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.4",
    "@gorhom/bottom-sheet": "^4",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/datetimepicker": "8.2.0",
    "@react-native-picker/picker": "2.9.0",
    "@react-navigation/bottom-tabs": "^7.3.4",
    "@react-navigation/native": "^7.1.0",
    "@react-navigation/stack": "^7.2.4",
    "@stomp/stompjs": "^7.1.1",
    "axios": "^1.8.4",
    "babel-plugin-inline-import": "^3.0.0",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "expo": "~52.0.42",
    "expo-av": "~15.0.2",
    "expo-blur": "~14.0.3",
    "expo-constants": "~17.0.8",
    "expo-haptics": "~14.0.1",
    "expo-image-picker": "~16.0.6",
    "expo-linear-gradient": "~14.0.2",
    "expo-location": "~18.0.10",
    "expo-status-bar": "~2.0.1",
    "mobile": "file:",
    "react": "18.3.1",
    "react-native": "0.76.9",
    "react-native-animatable": "^1.4.0",
    "react-native-autocomplete-input": "^5.5.6",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-maps": "1.18.0",
    "react-native-modal-datetime-picker": "^18.0.0",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.8.0",
    "react-native-toast-message": "^2.3.0",
    "socket.io-client": "^4.8.1",
    "sockjs-client": "^1.6.1"
  },
  "nyc": {
    "include": [
      "App.js",
      "App.jsx", 
      "App.ts",
      "App.tsx",
      "src/**/*.js",
      "src/**/*.jsx",
      "src/**/*.ts", 
      "src/**/*.tsx",
      "*.js",
      "*.jsx",
      "*.ts",
      "*.tsx"
    ],
    "exclude": [
      "node_modules/**",
      "**/*.test.js",
      "**/*.test.jsx",
      "**/*.test.ts",
      "**/*.test.tsx",
      "**/*.spec.js",
      "**/*.spec.jsx", 
      "**/*.spec.ts",
      "**/*.spec.tsx",
      "**/e2e/**",
      "coverage/**",
      "metro.config.js",
      "babel.config.js",
      "**/__tests__/**",
      "**/.expo/**"
    ],
    "extension": [
      ".js",
      ".jsx",
      ".ts",
      ".tsx"
    ],
    "reporter": [
      "html",
      "text",
      "lcov"
    ],
    "sourceMap": true,
    "instrument": true,
    "cache": false,
    "all": true
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@react-native-community/cli": "latest",
    "babel-plugin-istanbul": "^7.0.0",
    "cross-env": "^7.0.3",
    "metro-react-native-babel-transformer": "^0.77.0",
    "nyc": "^17.1.0",
    "react-native-dotenv": "^3.4.11"
  },
  "private": true
}
